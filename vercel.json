
{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://api.vercel.app/$1",
      "headers": {
        "x-forwarded-proto": "https"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
